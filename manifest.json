{
  "name": "Slack Workspace Users Profile Getter",
  "version": "1.0",
  "description": "Enable the extension, then visit the channel that you'd like to get its list of users.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "https://*.slack.com/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
